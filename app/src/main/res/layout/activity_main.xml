<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.herbert.demoapp.MainActivity">
    <Button
        android:id="@+id/bt_aidl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="aidl测试"/>
    <Button
        android:id="@+id/bt_contentprovider"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="获取手机联系人信息"
        app:layout_constraintTop_toBottomOf="@id/bt_aidl"/>
    <Button
        android:id="@+id/bt_cp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="内容提供者"
        app:layout_constraintTop_toBottomOf="@id/bt_contentprovider"/>
    <Button
        android:id="@+id/bt_launch"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="启动另一个App"
        app:layout_constraintTop_toBottomOf="@id/bt_cp"/>
</android.support.constraint.ConstraintLayout>
